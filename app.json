{
  "expo": {
    "name": "FitNity",
    "slug": "FitNity",
    "owner": "yuhenggoh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconFitNity.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/iconFitNity.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconFitNity.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fyp.FitNity"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "fitnity",
    "plugins": [
      "expo-web-browser",
      "expo-notifications",
      "expo-asset"
    ]
  }
}
